type int = __builtin_value__ "int-type";
type void = __builtin_value__ "void-type";
type bool = __builtin_value__ "bool-type";
bool true = __builtin_value__ "true";
bool false = __builtin_value__ "false";

// 'static' in Bread means the same as 'enum' in D basically
// everything in the global scope is automatically marked 'static'

function(void; int) print = __builtin_value__ "print";

int fib(int x) {
	if (x <= 1) {
		return x;
	}
	return fib(x - 1) + fib(x - 2);
}

void main() {
	print(fib(17));
	foot();
}

void foot() {
	print(5);
	return print(3);
}

